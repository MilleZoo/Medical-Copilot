{
  "name": "newmes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "cd .. && husky FrontEnd/.husky",
    "format": "prettier --cache --write .",
    "lint": "eslint --cache .",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,scss}": [
      "prettier --write"
    ]
